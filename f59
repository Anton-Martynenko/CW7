function findCaterer(budget, people){
  if (people < 1 ) {
    return -1;
  } else if (budget < people * 15) {
    return -1;
  } else if ( people > 60) {
    if (budget >= people * 30 * 0.8) {
      return 3;
    } else if (budget >= people * 20) {
    return 2;
  } else if (budget >= people * 15) {
    return 1;
  }
  } else if (budget >= people * 30) {
    return 3;
  } else if (budget >= people * 20) {
    return 2;
  } else if (budget >= people * 15) {
    return 1;
  } 
  
}
